{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "server={server};port={port};user={user};password={password};database={database}"
  },
  "AppSettings": {
    "UrlComum": "{url}",
    "UrlGestao": "{url}",
    "UrlApi": "{url}"
  },
  "JwtSettings": {
    "ExpiracaoHoras": "{expiracaoEmHoras(Inteiro)}",
    "Emissor": "{emissor}",
    "ComumValidoEm": "{comumValidoEm}",
    "GestaoValidoEm": "{gestaoValidoEm}",
    "CaminhoKeys": "{path}"
  },
  "EmailSettings": {
    "Nome": "Finansy",
    "Usuario": "{usuarioEmBase64}",
    "Senha": "{password}",
    "Servidor": "smtp.gmail.com",
    "Porta": 587
  },
  "UploadSettings": {
    "PublicBasePath": "{path}",
    "PrivateBasePath": "{path}"
  }
}
